{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"description": "Schema for the reddit credentials",
	"type": "object",
	"title": "OpenAICredentials",
	"required": [
		"api_key",
		"model_id"
	],
	"properties": {
		"api_key": {
			"type": "string",
			"description": "The API key for the OpenAI API"
		},
		"model_id": {
			"type": "string",
			"description": "The ID of the OpenAI model to use"
		}
	}
}